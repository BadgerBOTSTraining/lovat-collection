{
  "cli": {
    "version": ">= 7.2.0",
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "development-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "staging": {
      "env": {
        "EXPO_PUBLIC_API_URL": "https://lovat-server-staging.up.railway.app"
      }
    },
    "production": {}
  },
  "submit": {
    "production": {}
  }
}
